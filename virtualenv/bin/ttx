#!/home/ssaa/Git/pyAMOSA/virtualenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'fonttools==4.38.0','console_scripts','ttx'
__requires__ = 'fonttools==4.38.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('fonttools==4.38.0', 'console_scripts', 'ttx')()
    )
